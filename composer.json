{
    "name": "soundsmarket/nova-image-cropper",
    "description": "A Laravel Nova field tweeked by Sounds Market.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "SoundsMarket\\NovaImageCropper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoundsMarket\\NovaImageCropper\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
